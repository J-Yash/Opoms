    <servlet>
        <servlet-name>NewUserForm</servlet-name>
        <servlet-class>WorkOfTheForms.NewUserForm</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ForgotPassword</servlet-name>
        <servlet-class>WorkOfTheForms.ForgotPassword</servlet-class>
