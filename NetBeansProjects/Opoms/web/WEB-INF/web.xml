        <servlet-class>WorkOfTheForms.NewProductEntry</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddNewCompany</servlet-name>
        <servlet-class>WorkOfTheForms.AddNewCompany</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FileUpload</servlet-name>
        <servlet-class>WorkOfTheForms.FileUpload</servlet-class>
