    </servlet>
    <servlet>
        <servlet-name>IssueReelDetailsSubmit</servlet-name>
        <servlet-class>WorkOfTheForms.IssueReelDetailsSubmit</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>NewUserForm</servlet-name>
        <url-pattern>/NewUser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ForgotPassword</servlet-name>
        <url-pattern>/gettingTheWord</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginPage</servlet-name>
        <url-pattern>/LoginPage</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewProductEntry</servlet-name>
        <url-pattern>/NewProduct</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddNewCompany</servlet-name>
        <url-pattern>/NewCompany</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FileUpload</servlet-name>
        <url-pattern>/FileUpload</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetProductNameForPo</servlet-name>
        <url-pattern>/GetProductName</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
