        <servlet-name>IssueReelDetailsSubmit</servlet-name>
        <url-pattern>/IssueReelDetails</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
